{
    "version": 2,
    "buildCommand": "npx prisma generate",
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/index.js"
        },
        {
            "src": "/p/(.*)",
            "dest": "src/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "public/$1"
        }
    ]
}