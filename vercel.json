{
    "version": 2,
    "buildCommand": "npx prisma generate",
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/src/index.js"
        },
        {
            "source": "/p/(.*)",
            "destination": "/src/index.js"
        }
    ]
}